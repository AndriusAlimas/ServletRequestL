<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
 
  <display-name>ServletRequestL</display-name>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
   <listener>
  	<listener-class>com.example.listener.MyServletRequestListener</listener-class>
  </listener>
  
  <servlet>
  	<servlet-name>Servletukas</servlet-name>
  	<servlet-class>com.example.servlet.MainServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Servletukas</servlet-name>
  	<url-pattern>/servlet.do</url-pattern>
  </servlet-mapping>
  
</web-app>